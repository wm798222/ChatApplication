				&#xD;
			}&#xD;
&#xD;
			@Override&#xD;
			public void refreshNotYetConnectedUser(Collection&lt;IUser> collection) {&#xD;
				view.refreshNotYetConnectedUser(collection);&#xD;
				&#xD;
			}&#xD;
&#xD;
&#xD;
		};&#xD;
		&#xD;
