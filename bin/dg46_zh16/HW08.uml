		//buttonPnl.add(btnSendFile);&#xD;
		&#xD;
		GridBagConstraints gbc_btnHackMessage = new GridBagConstraints();&#xD;
		gbc_btnHackMessage.fill = GridBagConstraints.HORIZONTAL;&#xD;
		gbc_btnHackMessage.gridx = 3;&#xD;
